<%- include('./header')-%>

    <body>
        <div class="container mt-3">
            <div class="jumbotron">
                <h1 style="color:black mt-3;"></h1>
                <h2 style="color:black mt-3;">Iniciar sesión</h2>

                <div class="form-group">
                    <label for="exampleInputEmail1" class="form-label">Usuario:</label>
                    <input type="email" class="form-control" id="username" aria-describedby="emailHelp"
                        name="username" required>
                </div>

                <div class="form-group">
                    <label for="exampleInputPassword1" class="form-label">Contraseña</label>
                    <input type="password" class="form-control" id="password" name="password" required>
                </div>

                <button class="btn btn-primary mt-3 mb-5" id="login">Enviar</button>

                <button class="btn  btn-outline-secondary mt-3 mb-5" id="btn">Registrar</button>
            </div>
        </div>
        <script src="./javascript/iniciar-sesion.js"></script>

    </body>

